<%= render partial: "layouts/navbar" %>
<div class="l-container l-content">
  <div class="l-fluid-fixed">
    <div class="sc-border-light-right l-main">
      <div class="l-tabs">
        <div class="streamExploreTabs">
          <%= render partial: "layouts/categories" %>
          <div>
            <% @likes.each do |like| %>
              <iframe width="100%"
                      height="124"
                      scrolling="no"
                      frameborder="no"
                      src="https://w.soundcloud.com/player/?url=<%= like[:uri] %>&amp;color=0aaaff&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>